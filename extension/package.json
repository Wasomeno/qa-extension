    "install-dev-deps": "node scripts/install-dev-deps.js",
    "hot-reload": "node scripts/dev-reload.js",
    "check-deps": "node -e \"try { require('chokidar'); require('ws'); require('concurrently'); } catch(e) { console.error('âŒ Hot reload dependencies missing. Installing...'); process.exit(1); }\"",
    "build": "webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
